{
    "extends": "airbnb-base/legacy",
    "plugins": [
    ],
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "indent": ["error", 2],
      "max-len": [2, 120, 2],
      "no-underscore-dangle": ["error", { "allow": ["_id", "_extend"] }],
      "comma-dangle": ["error", "always-multiline"],
      "no-shadow": ["error", { "allow": ["err"] }],
      "no-multi-str": 0,
      "global-require": 0,
      "new-cap": 0,
      "newline-per-chained-call": 0,
      "func-names": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-console": 0
    }
}
